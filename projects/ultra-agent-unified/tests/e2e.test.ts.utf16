describe("Ultra Agent Unified E2E", () => { test("should load manifest", async () => { const manifest = require("./manifest.json"); expect(manifest.name).toBe("ultra-agent-unified"); expect(manifest.core_compatibility).toBe("100%"); }); test("should have API routes", () => { const routes = require("./api/routes"); expect(routes["/health"]).toBeDefined(); expect(routes["/api/jobs"]).toBeDefined(); expect(routes["/api/memory"]).toBeDefined(); }); test("should have agents", () => { const planner = require("./agents/planner"); const executor = require("./agents/executor"); const verifier = require("./agents/verifier"); expect(planner.plan).toBeDefined(); expect(executor.execute).toBeDefined(); expect(verifier.verify).toBeDefined(); }); test("should have memory system", () => { const memory = require("./memory/index"); expect(memory.initialize).toBeDefined(); expect(memory.createMemory).toBeDefined(); }); test("should have UI pages", () => { const pages = require("./ui/pages/index"); expect(pages).toBeDefined(); }); test("should have permissions", () => { const permissions = require("./permissions.json"); expect(permissions.default).toBe("deny"); expect(permissions.roles).toBeDefined(); }); });
